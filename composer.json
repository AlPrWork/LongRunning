{
    "name": "long-running/long-running",
    "type": "library",
    "description": "Tools for working with long-running processes",
    "homepage": "http://github.com/LongRunning/LongRunning",
    "license": "MIT",
    "authors": [
        {
            "name": "Matthias Noback",
            "email": "matthiasnoback@gmail.com",
            "homepage": "http://php-and-symfony.matthiasnoback.nl"
        }
    ],
    "require": {
        "php": ">=5.4"
    },
    "require-dev": {
        "doctrine/doctrine-bundle": "~1.0",
        "doctrine/orm": "~2.3",
        "phpunit/phpunit": "~4.0",
        "satooshi/php-coveralls": "~0.6",
        "simple-bus/rabbitmq-bundle-bridge": "~2.1",
        "symfony/form": "~2.3",
        "symfony/framework-bundle": "~2.3",
        "symfony/monolog-bundle": "~2.3",
        "symfony/swiftmailer-bundle": "~2.3",
        "symfony/validator": "~2.3",
        "twig/twig": "~1.18"
    },
    "autoload": {
        "psr-4" : { "LongRunning\\" : "src/" }
    },
    "autoload-dev": {
        "psr-4": { "LongRunning\\Tests\\": "tests" }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
